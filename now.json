{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/rap2/(.*)", "dest": "http://rap2api.taobao.org/$1" },
    { "src": "^/(js|css|img)/.*", "headers": { "cache-control": "max-age=31536000, immutable" }},
    { "src": ".*", "dest": "/" }
  ],
  "regions": ["hnd1"]
}
